<section class="todoapp">
			<header class="header">
				<h1>todos</h1>
				<%= form_for(@list) do |f| %>
					<%= f.text_field :name %>
					<%= f.submit %>
				<% end %>
				</header>
			<section class="main">
				<input class="toggle-all" type="checkbox">
				<label for="toggle-all"> Mark all as complete</label>
				<ul class="todo-list">
				<% @lists.each do |list| %>
					<li>
						<div class="view">
						<label><%= link_to list.name, list_path(list) %></label>
						</div>
					</li>
					<% end %>
				</ul>
			</section>
			</section>
